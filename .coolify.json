{
  "version": "1.0",
  "projectName": "Generación de Documentación",
  "buildpack": "docker",
  "dockerfile": "./Dockerfile",
  "port": 3000,
  "startCommand": "node .output/server/index.mjs",
  "healthCheck": {
    "type": "http",
    "path": "/",
    "interval": 30,
    "timeout": 3,
    "retries": 3
  },
  "environment": {
    "NODE_ENV": "production"
  }
}
